    "os.chdir(output_path)\n",
    "for folder in file_list:\n",
    "    if dir_name in folder:\n",
    "        \n",
    "        replicate_counter+=1\n",
    "        phycorder_csv_list = []\n",
    "        gon_phy_csv_list = []\n",
    "        os.chdir(folder)\n",
    "        \n",
    "        print(\"CURRENT WORKING OUTPUT FOLDER:\", folder)\n",
    "        stats_files = os.listdir(\".\")\n",
    "        for file in stats_files:\n",
    "            if \".csv\" in file:\n",
    "                if \"phycorder_taxon_stats\" in file:\n",
    "                    phycorder_csv_list.append(file)\n",
    "                elif \"gon_phy_taxon_stats\" in file:\n",
    "                    gon_phy_csv_list.append(file)\n",
    "        \n",
    "        \n",
    "        phycorder_csv_list.sort(key=lambda f: int(''.join(filter(str.isdigit, f))))\n",
